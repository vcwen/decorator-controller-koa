{
  "name": "decorator-controller-koa",
  "version": "1.0.0",
  "description": "decorator of controller for koa2",
  "main": "index.js",
  "jest": {
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/test/"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "test/.*\\.(test|spec)\\.(tsx?)$",
    "testEnvironment": "node",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "repository": "https://github.com/vcwen/decorator-controller-koa.git",
  "author": "vincent.wen <wenwei1202@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/koa-bodyparser": "^4.2.0",
    "@types/lodash": "^4.14.104",
    "@types/node": "^9.4.5",
    "jest": "^22.4.3",
    "ts-jest": "^22.0.4",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "@types/boom": "^7.1.1",
    "@types/koa-router": "^7.0.27",
    "boom": "^7.1.1",
    "decamelize": "^2.0.0",
    "immutable": "^4.0.0-rc.9",
    "koa": "^2.5.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.5",
    "pluralize": "^7.0.0",
    "reflect-metadata": "^0.1.12",
    "superstruct": "^0.5.0"
  }
}
